{
    "name": "chrismou/lastfm",
    "description": "A simple last.fm api wrapper for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Chrisostomou",
            "email": "dev@mou.me"
        }
    ],
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "*",
        "vectorface/dunit": "^2.1",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Chrismou\\LastFm\\": "src/",
            "Chrismou\\LastFm\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
